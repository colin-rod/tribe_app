{
  "functions": {
    "src/app/api/webhooks/*/route.ts": {
      "maxDuration": 30,
      "maxRequestBodySize": "10mb"
    }
  },
  "headers": [
    {
      "source": "/api/webhooks/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ]
}